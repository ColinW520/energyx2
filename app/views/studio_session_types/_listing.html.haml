.row
  .col-sm-12
    .card.card-outline-primary.table-card
      .table-responsive
        %table.table#sortable
          %thead.thead-inverse
            %th Promo Image
            %th= smart_listing.sortable 'Name', :name
            %th Description
            %th= smart_listing.sortable 'Updated', :updated_at
            %th.text-center Link
            %th
          %tbody#studio_session_types
            - smart_listing.collection.each do |studio_session_type|
              = render studio_session_type
        %br
%br
.row
  .col-sm-12
    .pull-right
      = smart_listing.paginate
      = smart_listing.pagination_per_page_links
