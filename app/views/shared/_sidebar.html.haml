#sidebar
  .nav-side-menu
    .brand EXF
    %i.fa.fa-bars.fa-2x.toggle-btn{"data-target" => "#menu-content", "data-toggle" => "collapse"}
    .menu-list
      %ul#menu-content.menu-content.collapse.out
        %a{href: dashboard_path}
          %li{class: @subnav == 'dashboard' ? 'active' : nil}
            %i.fa.fa-dashboard.fa-lg
            Dashboard

        - if current_user.organization_id.present?
          %a{href: organization_users_path(current_user.organization)}
            %li{class: @subnav == 'users' ? 'active' : nil}
              %i.fa.fa-users
              Users

        %a{href: studio_session_types_path}
          %li{class: @subnav == 'studio_session_types' ? 'active' : nil}
            %i.fa.fa-star
            Classes

        %a{href: studio_sessions_path}
          %li{class: @subnav == 'studio_sessions' ? 'active' : nil}
            %i.fa.fa-calendar
            Schedule

        %a{href: destroy_user_session_path, method: :delete}
          %li
            %i.fa.fa-sign-out.fa-lg
            Sign Out
